<?xml version="1.0" encoding="utf-8"?>
     
<RelativeLayout
	android:id="@+id/RelativeLayout01"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="fill_parent"
    android:layout_width="fill_parent">

    <AutoCompleteTextView
    	android:layout_width="fill_parent"
     	android:layout_height="wrap_content"
     	android:hint="@string/help"
     	android:id="@+id/StopEditText"
     	android:textColor="#000000"
     	android:layout_marginTop="25px"/>

    <Button
     	style="@style/ButtonStyle"
     	android:layout_below="@+id/StopEditText"
     	android:layout_alignParentRight="true"
     	android:text="@string/save"
     	android:id="@+id/SaveButton01"></Button>

    <Button
     	style="@style/ButtonStyle"
     	android:layout_below="@+id/StopEditText"
     	android:layout_toLeftOf="@+id/SaveButton01"
     	android:text="@string/search"
     	android:id="@+id/SearchButton01"></Button>

    <ListView
     	android:layout_width="fill_parent"
     	android:layout_height="wrap_content"
     	android:layout_below="@+id/SearchButton01"
     	android:id="@id/android:list"
     	android:layout_marginTop="25px"></ListView>
     	


</RelativeLayout>
     	

        


